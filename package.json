{
  "name": "frontend",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test:units": "vitest run",
    "test:units:watch": "vitest watch",
    "test:integration": "npx playwright test --config tests/integration",
    "test:e2e": "npx playwright test --config tests/e2e",
    "test": "npm run test:units && npm run test:integration && npm run test:e2e --skip-build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "format:check": "prettier --check --plugin-search-dir=. ."
  },
  "engines": {
    "node": "^16.15.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@sveltejs/adapter-node": "1.0.0-next.101",
    "@sveltejs/kit": "1.0.0-next.554",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "esbuild": "^0.13.4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fast-deep-equal": "^3.1.3",
    "jsdom": "^20.0.2",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.55.1",
    "svelte-check": "^2.7.1",
    "svelte-headless-table": "^0.17.2",
    "svelte-highlight": "^6.2.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "vite-plugin-simple-json-server": "^0.6.1",
    "vitest": "^0.24.3",
    "vitest-github-actions-reporter": "^0.8.3"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "@trading-strategy-ai/web-top-node": "^0.1.0-beta.9",
    "@tryghost/content-api": "^1.11.5",
    "assert-ts": "^0.3.4",
    "cheerio": "^1.0.0-rc.12",
    "cookie": "^0.5.0",
    "d3-array": "^3.2.1",
    "d3-scale": "^4.0.2",
    "datatables.net-dt": "^1.11.3",
    "datatables.net-responsive-bs4": "^2.2.9",
    "date-fns": "^2.29.1",
    "dd-trace": "^3.12.1",
    "ethers": "^5.6.4",
    "node-cache": "^5.1.2",
    "plotly.js-finance-dist": "^2.16.3",
    "svelte-fsm": "^1.1.2",
    "svelte-inview": "^3.0.0",
    "svelte-spinner": "^2.0.2",
    "svelte-tradingview-widget": "^2.3.0",
    "ts-custom-error": "^3.2.0",
    "typesense": "^1.4.4",
    "web3": "^1.6.0"
