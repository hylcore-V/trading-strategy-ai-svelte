{
  "name": "frontend",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "format:check": "prettier --check --plugin-search-dir=. .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "npm test -- --watch",
    "ssr:node": "vite build"
  },
  "engines": {
    "node": "^16.15.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.svg$": "jest-transform-stub",
      "^.+\\.png$": "jest-transform-stub",
      "^.+\\.(ts|tsx|js|jsx)$": [
        "esbuild-jest"
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/cypress/"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "moduleNameMapper": {
      "^\\$lib(.*)$": "<rootDir>/src/lib$1",
      "^\\$app(.*)$": "<rootDir>/.svelte-kit/dev/runtime/app$1"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@sveltejs/adapter-node": "1.0.0-next.87",
    "@sveltejs/kit": "1.0.0-next.449",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.0",
    "@types/cookie": "^0.4.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "assert-ts": "^0.3.4",
    "babel-jest": "^26.5.2",
    "datatables.net-dt": "^1.11.3",
    "datatables.net-responsive-bs4": "^2.2.9",
    "esbuild": "^0.13.4",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "gridjs": "^5.0.2",
    "gridjs-svelte": "^2.1.1",
    "jest": "^26.5.2",
    "jest-transform-stub": "^2.0.0",
    "jest-transform-svelte": "^2.1.1",
    "jquery": "^3.6.0",
    "postcss-font-display": "^0.3.0",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.7.1",
    "svelte-highlight": "^5.1.2",
    "svelte-jester": "^1.1.5",
    "svelte-preprocess": "^4.10.6",
    "ts-jest": "^26.4.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  },
  "dependencies": {
    "@fontsource/fira-mono": "^4.2.2",
    "@lukeed/uuid": "^2.0.0",
    "@metamask/detect-provider": "^1.2.0",
    "@trading-strategy-ai/web-top-node": "^0.1.0-beta.9",
    "@tryghost/content-api": "^1.5.13",
    "cookie": "^0.4.1",
    "date-fns": "^2.29.1",
    "ethers": "^5.6.4",
    "gulp": "^4.0.2",
    "svelte-fsm": "^1.1.2",
    "svelte-inview": "^3.0.0",
    "svelte-spinner": "^2.0.2",
    "svelte-tradingview-widget": "^2.3.0",
    "ts-custom-error": "^3.2.0",
    "typesense": "^1.2.1",
    "web3": "^1.6.0"
  },
  "optionalDependencies": {
    "chartiq": "github:tradingstrategy-ai/chartiq-dist#v8.7.0"
  }
}
