{
  "name": "frontend",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:integration": "npx playwright test --config tests/integration",
    "test:e2e": "npx playwright test --config tests/e2e",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e --skip-build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "format:check": "prettier --plugin-search-dir=. . --check"
  },
  "engines": {
    "node": "^18.14.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "devDependencies": {
    "@playwright/test": "^1.32.3",
    "@sveltejs/adapter-node": "^1.2.3",
    "@sveltejs/kit": "^1.15.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cryptocurrency-icons": "^0.18.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fast-deep-equal": "^3.1.3",
    "jsdom": "^21.1.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-headless-table": "^0.17.3",
    "svelte-highlight": "^7.2.1",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-simple-json-server": "^0.6.2",
    "vitest": "^0.30.1",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "dependencies": {
    "@sentry/node": "^7.47.0",
    "@trading-strategy-ai/web-top-node": "^0.1.0-beta.9",
    "@tryghost/content-api": "^1.11.8",
    "assert-ts": "^0.3.4",
    "cheerio": "^1.0.0-rc.12",
    "cookie": "^0.5.0",
    "d3-array": "^3.2.3",
    "d3-scale": "^4.0.2",
    "date-fns": "^2.29.3",
    "dd-trace": "^3.17.1",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "svelte-fsm": "^1.2.0",
    "svelte-inview": "^4.0.0",
    "svelte-spinner": "^2.0.2",
    "typesense": "^1.5.3"
  },
  "optionalDependencies": {
    "chartiq": "github:tradingstrategy-ai/chartiq-dist#v8.8.4"
  }
}
